{
    "name": "orchestra/facile",
    "description": "Facile Component for Orchestra Platform",
    "homepage": "http://orchestraplatform.com/docs/latest/components/facile/",
    "keywords": ["orchestra-platform", "orchestral", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Facile\\" : "src/"
        }
    },
    "require": {
        "php": ">=5.5.0",
        "illuminate/contracts": "~5.1.0",
        "orchestra/support": "~3.1.0",
        "spatie/array-to-xml": "~2.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.3",
        "illuminate/container": "~5.1.0",
        "illuminate/database": "~5.1.0",
        "illuminate/http": "~5.1.4",
        "illuminate/pagination": "~5.1.0"
    },
    "suggest": {
        "laravel/framework": "Allow using orchestra/facile component outside of Orchestra Platform (~5.1).",
        "orchestra/foundation": "Allow using orchestra/facile with Orchestra Platform (~3.1)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.2-dev"
        }
    },
    "minimum-stability": "dev"
}
